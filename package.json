{
  "name": "incognito-js",
  "version": "1.0.0",
  "description": "Incognito Javascript library",
  "main": "build/node",
  "repository": {
    "type": "git",
    "url": "https://github.com/incognitochain/sdk-v2"
  },
  "scripts": {
    "prestart": "yarn",
    "start": "yarn build",
    "lint": "eslint src",
    "clean:build": "rimraf build",
    "build:web:module": "rimraf build/web/module && yarn lint && NODE_ENV='production' TARGET='web:module' webpack --progress",
    "build:web:browser": "rimraf build/web/browser && yarn lint && NODE_ENV='production' TARGET='web:browser' webpack --progress",
    "build:node": "rimraf build/node && yarn lint && NODE_ENV='production' TARGET='node' webpack --progress",
    "build": "yarn build:web:browser",
    "build:dev": "yarn build:dev:node && yarn build:dev:web:browser && yarn build:dev:web:module",
    "build:dev:node": "rimraf build/node && TARGET='node' webpack --progress",
    "build:dev:web:browser": "rimraf build/web/browser && TARGET='web:browser' webpack --progress",
    "build:dev:web:module": "rimraf build/web/module && TARGET='web:module' webpack --progress",
    "test:browser": "copyfiles -s privacy.wasm ./sample/ && http-server -o sample/index.html",
    "test:node": "node sample/test-node.js",
    "stats": "TARGET='node' webpack --json > stats.json",
    "prepublish": "npm run build"
  },
  "author": "vuong@autonomous.nyc",
  "license": "ISC",
  "dependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/circular-json": "^0.4.0",
    "@types/crypto-js": "^3.1.43",
    "@types/cryptojs": "^3.1.29",
    "@types/elliptic": "^6.4.12",
    "@types/lodash": "^4.14.149",
    "@types/pbkdf2": "^3.0.0",
    "axios": "^0.19.0",
    "circular-json": "^0.5.9",
    "crypto-js": "3.1.9-1",
    "fs": "^0.0.1-security",
    "http-server": "^0.12.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.0",
    "path": "^0.12.7",
    "sha3": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/bn.js": "^4.11.6",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.1.1",
    "errorcode-loader": "^0.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "file-loader": "^5.0.2",
    "rimraf": "^3.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.7.5",
    "typescript-transform-paths": "^1.1.14",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  }
}
